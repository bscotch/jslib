{
  "name": "@bscotch/eslint-plugin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:dev": "tsc --build",
    "clean:dev": "rimraf build dist *.tsbuildinfo **/*.tsbuildinfo",
    "test:dev": "mocha --config ../../config/.mocharc.cjs --forbid-only=false --parallel=false --timeout=9999999999",
    "watch:dev": "tsc --build --watch"
  },
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/utils": "^5.48.2",
    "mocha": "^10.2.0",
    "rimraf": "^4.1.0",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  }
}
